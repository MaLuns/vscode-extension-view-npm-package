{
	"name": "npm-package-view",
	"displayName": "view npm package",
	"description": "view npm package in vscode",
	"version": "0.0.6",
	"repository": {
		"type": "git",
		"url": "https://github.com/MaLuns/vscode-extension-view-npm-package"
	},
	"publisher": "mal",
	"author": {
		"name": "白云苍狗"
	},
	"keywords": [
		"npm",
		"package",
		"view package",
		"view node package"
	],
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"icon": "icons/icon128x128.png",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "npm.packageview.search",
				"title": "search npm package",
				"icon": "$(filter)"
			},
			{
				"command": "npm.packageview.switchVersion",
				"title": "select package version",
				"icon": "$(list-filter)"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "npm.packageview.search",
					"when": "false"
				},
				{
					"command": "npm.packageview.switchVersion",
					"when": "false"
				}
			],
			"view/title": [
				{
					"command": "npm.packageview.search",
					"when": "view == views.npm.search.list",
					"group": "navigation"
				},
				{
					"command": "npm.packageview.switchVersion",
					"when": "view == views.npm.package.list",
					"group": "navigation"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "npm-package-view",
					"title": "npm package",
					"icon": "icons/logo.svg"
				}
			]
		},
		"views": {
			"npm-package-view": [
				{
					"name": "npm list",
					"id": "views.npm.search.list"
				},
				{
					"name": "package file",
					"id": "views.npm.package.list"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "views.npm.search.list",
				"contents": "Please click the filter icon in the upper right corner to query the NPM package"
			},
			{
				"view": "views.npm.package.list",
				"contents": "Select the NPM package for the query"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
