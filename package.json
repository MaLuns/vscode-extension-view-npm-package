{
	"name": "npm.packageview",
	"displayName": "npm package view",
	"description": "npm package view",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "npm.packageview.search",
				"title": "search npm package",
				"icon": "$(filter)"
			},
			{
				"command": "npm.packageview.selectVersion",
				"title": "select package version",
				"icon": "$(list-filter)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "npm.packageview.search",
					"when": "view == views.npm.search.list",
					"group": "navigation"
				},
				{
					"command": "npm.packageview.selectVersion",
					"when": "view == views.npm.package.list",
					"group": "navigation"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "npm-package-view",
					"title": "npm package",
					"icon": "icons/logo.svg"
				}
			]
		},
		"views": {
			"npm-package-view": [
				{
					"name": "npm list",
					"id": "views.npm.search.list"
				},
				{
					"name": "package file",
					"id": "views.npm.package.list"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}